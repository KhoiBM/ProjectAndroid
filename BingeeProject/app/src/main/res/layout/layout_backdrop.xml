<?xml version="1.0" encoding="utf-8"?>

<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <com.google.android.material.button.MaterialButton
        android:id="@+id/nav_btn_go_home"
        style="@style/Widget.Bingee.Button.TextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO HOME"
        android:textColor="@color/textColorSecondary"
        android:textSize="8pt" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/nav_btn_ingredients"
        style="@style/Widget.Bingee.Button.TextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="INGREDIENTS"
        android:textColor="@color/textColorSecondary"
        android:textSize="8pt" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/nav_btn_furniture"
        style="@style/Widget.Bingee.Button.TextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FURNITURE"
        android:textColor="@color/textColorSecondary"
        android:textSize="8pt" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/nav_btn_new"
        style="@style/Widget.Bingee.Button.TextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NEW"
        android:textColor="@color/textColorSecondary"
        android:textSize="8pt" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/nav_btn_sale"
        style="@style/Widget.Bingee.Button.TextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SALE"
        android:textColor="@color/textColorSecondary"
        android:textSize="8pt" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/nav_btn_cart"
        style="@style/Widget.Bingee.Button.TextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CART"
        android:textColor="@color/textColorSecondary"
        android:textSize="8pt" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/nav_btn_about"
        style="@style/Widget.Bingee.Button.TextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ABOUT"
        android:textColor="@color/textColorSecondary"
        android:textSize="8pt" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/nav_btn_contact"
        style="@style/Widget.Bingee.Button.TextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CONTACT"
        android:textColor="@color/textColorSecondary"
        android:textSize="8pt" />

    <View
        android:layout_width="100dp"
        android:layout_height="1dp"
        android:layout_margin="16dp"
        android:background="@color/textColorSecondary" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/nav_btn_my_account"
        style="@style/Widget.Bingee.Button.TextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textColorSecondary"
        android:textSize="8pt" />
</merge>
